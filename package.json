{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "copy":  "cpy '.env.example' '.' --rename=.env --no-overwrite && cpy 'serviceAccountKey.json.example' '.' --rename=serviceAccountKey.json --no-overwrite  ",
    "clean": "rimraf dist",
    "pretty": "prettier ./dist/*.js ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts  --color --write",
    "lint": "yarn run pretty && tsc --noEmit && eslint  --ext .ts src/ --color --fix",
    "build:babel": "NODE_ENV=production babel src --out-dir dist --extensions .ts --source-maps inline",
    "build": "yarn run clean && yarn run build:babel && yarn run lint",
    "start": "yarn run build && node dist/index.js",
    "dev": "NODE_ENV=development nodemon --watch src -e ts --exec ts-node ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
